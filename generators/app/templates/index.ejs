const express = require('express');
const app = express();
const port = process.env.PORT || <%= port %>;
<%- importRoutes %>

app.use(express.json());

app.get('/', (req, res) => {
  res.json({message: 'alive'});
});


<%- registerRoutes %>

app.listen(port, () => {
  console.log(`Example app listening at http://localhost:${port}`);
});
